{
    "env": {
      "browser": true,
      "es2022": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react-hooks/recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react", "react-hooks"],
    "rules": {
      "react-hooks/rules-of-hooks": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
}